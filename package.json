{
    "name": "@sonwonjae/react-markdownarea",
    "description": "react markdownarea",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sonwonjae/react-markdownarea.git"
    },
    "keywords": [
        "markdown"
    ],
    "author": "sonwonjae",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sonwonjae/react-markdownarea/issues"
    },
    "homepage": "https://github.com/sonwonjae/react-markdownarea#readme",
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        },
        "./styles": "./dist/style.css"
    },
    "files": [
        "/dist"
    ],
    "scripts": {
        "dev": "vite",
        "clean": "rm -rf ./dist",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prepack": "npm run build"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.10",
        "@types/node": "^20.10.0",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react": "^4.2.0",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-turbo": "^2.0.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "typescript": "^5.2.2",
        "vite": "^5.0.0",
        "vite-plugin-dts": "^3.6.3"
    }
}
